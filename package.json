{
  "name": "fws-navigation",
  "version": "1.0.0",
  "description": "A nested navigation module",
  "main": "dist/menu.js",
  "watch": {
    "browserify": "src/js/*.js"
  },
  "scripts": {
    "watch": "npm-watch",
    "pretest": "jshint --reporter=node_modules/jshint-stylish src/**/*.js",
    "test": "karma start",
    "watch:sass": "node-sass -w src/scss/styles.scss example/menu.css",
    "build:sass": "node-sass --output-style compressed src/scss/styles.scss example/menu.css",
    "browserify": "browserify src/js/menu.js --standalone Menu | derequire | uglifyjs -mc > dist/menu.js",
    "postbrowserify": "ncp dist/menu.js example/js/menu.js && npm run browserify:example",
    "browserify:example": "browserify example/js/index.js -o example/bundle.js",
    "serve": "browser-sync start --server example --files 'example/**/*'",
    "mkdist": "node build/create-dist",
    "prebuild": "npm run mkdist && npm run build:sass",
    "build": "npm run browserify",
    "prestart": "npm run mkdist && npm run build:sass",
    "start": "npm run watch & npm run watch:sass & npm run browserify & npm run serve",
    "inspect": "browserify --full-paths example/js/index.js | uglifyjs -mc | Discify --open",
    "publish:demo": "npm run build && gh-pages -d example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/USFWS/navigation.git"
  },
  "keywords": [
    "USFWS",
    "Navigation",
    "Responsive"
  ],
  "author": "@r0yhewitt",
  "license": "SAX-PD",
  "bugs": {
    "url": "https://github.com/USFWS/navigation/issues"
  },
  "homepage": "https://github.com/USFWS/navigation#readme",
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "derequire": "^2.0.3",
    "gh-pages": "^0.11.0",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.21",
    "karma-browserify": "^5.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.0",
    "karma-safari-launcher": "^0.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "ncp": "^2.0.0",
    "node-sass": "^3.7.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "is-dom": "^1.0.5",
    "lodash.defaults": "^4.0.1",
    "lodash.foreach": "^4.1.1",
    "parent-node-selector": "^0.1.1",
    "tabbable": "^1.0.0"
  }
}
